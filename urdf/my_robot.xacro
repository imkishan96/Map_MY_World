<?xml version='1.0'?>

<robot name="test_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo" />
  <xacro:property name="wheel_mass" value="1.0"/>
  <xacro:property name="chassis_mass" value="10"/>
  <xacro:property name="wheel_radius" value="0.075"/>

  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 ${wheel_radius}" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>
  
  <link name="chassis">
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="${chassis_mass}"/>
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.3 0.2 0.05"/>
      </geometry>
      <material name="">
        <color rgba="1.0 0.0 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.3 0.2 0.05"/>
      </geometry>
    </collision>

  </link>

  <joint name="chassis__front" type="fixed">
    <origin xyz="0.15 0.0 0.0" rpy="0.0 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="front"/>
    <axis xyz="0.0 0.0 0.0"/>
    <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
  </joint>

  <link name="front">
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 -1.5707"/>
      <mass value="1"/>
      <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
    </inertial>
    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 -1.5707"/>
      <geometry>
        <mesh filename="package://my_robot/meshes/Front.dae"/>
      </geometry>
      <material name="">
        <color rgba="1.0 0.0 0.0 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.1 0.2 0.05"/>
      </geometry>
    </collision>
  </link>

  <joint name="rear_left_wheel_hinge" type="continuous">
    <origin xyz="-0.1 0.15 0.0" rpy="0.0 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="rear_left_wheel"/>
    <axis xyz="0.0 1 0.0"/>
    <limit lower="0.0" upper="0.0" effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <link name="rear_left_wheel">

    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707"/>
      <mass value="${wheel_mass}"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>

    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="0.05"/>
      </geometry>
      <material name="">
        <color rgba="0.0 0.0 0.0 1.0"/>
      </material>
    </visual>

    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="0.05"/>
      </geometry>

      <surface>
        <friction>
          <ode>
            <mu>100000.0</mu>
            <mu2>100000.0</mu2>
          </ode>
        </friction>
        <contact>
          <ode>
            <min_depth>0.01</min_depth>
          </ode>
        </contact>
      </surface>

    </collision>

  </link>

  <joint name="front_left_wheel_hinge" type="continuous">
    <origin xyz="0.1 0.15 0.0" rpy="0.0 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="front_left_wheel"/>
    <axis xyz="0.0 1 0.0"/>
    <limit lower="0.0" upper="0.0" effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <link name="front_left_wheel">

    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707"/>
      <mass value="${wheel_mass}"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>

    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="0.05"/>
      </geometry>
      <material name="">
        <color rgba="0.0 0.0 0.0 1.0"/>
      </material>
    </visual>

    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="0.05"/>
      </geometry>
    </collision>

  </link>

  <joint name="rear_right_wheel_hinge" type="continuous">
    <origin xyz="-0.1 -0.15 0.0" rpy="0.0 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="rear_right_wheel"/>
    <axis xyz="0.0 1 0.0"/>
    <limit lower="0.0" upper="0.0" effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>  
  </joint>

  <link name="rear_right_wheel">

    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707"/>
      <mass value="${wheel_mass}"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>

    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="0.05"/>
      </geometry>
      <material name="">
        <color rgba="0.0 0.0 0.0 1.0"/>
      </material>
    </visual>

    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="0.05"/>
      </geometry>

      <surface>
        <friction>
          <ode>
            <mu>100000.0</mu>
            <mu2>100000.0</mu2>
          </ode>
        </friction>
        <contact>
          <ode>
            <min_depth>0.01</min_depth>
          </ode>
        </contact>
      </surface>
    </collision>

  </link>

  <joint name="front_right_wheel_hinge" type="continuous">
    <origin xyz="0.1 -0.15 0.0" rpy="0.0 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="front_right_wheel"/>
    <axis xyz="0.0 1 0.0"/>
    <limit lower="0.0" upper="0.0" effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>  
  </joint>

  <link name="front_right_wheel">

    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707"/>
      <mass value="${wheel_mass}"/>
      <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
    </inertial>

    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="0.05"/>
      </geometry>
      <material name="">
        <color rgba="0.0 0.0 0.0 1.0"/>
      </material>
    </visual>

    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 1.5707 1.5707"/>
      <geometry>
        <cylinder radius="${wheel_radius}" length="0.05"/>
      </geometry>
    </collision>

  </link>

  <joint name="camera_joint" type="fixed">
    <origin xyz="0.15 0.0 0.05" rpy="0.0 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="camera"/>
    <axis xyz="0.0 1 0.0"/>
    <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
  </joint>

  <link name="camera">

    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="0.1"/>
      <inertia ixx="0.05" ixy="0.0" ixz="0.0" iyy="0.05" iyz="0.0" izz="0.05"/>
    </inertial>

    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
      <material name="">
        <color rgba="1.0 0.65 0.0 1.0"/>
      </material>
    </visual>

    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.05 0.05 0.05"/>
      </geometry>
    </collision>
    
  </link>

  <joint name="camera_optical_joint" type="fixed">
    <origin xyz="0.0 0.0 0.0" rpy="-1.5707 0.0 -1.5707"/>
    <parent link="camera"/>
    <child link="camera_link_optical"/>
  </joint>

  <link name="camera_link_optical">
  </link>

  <joint name="hokuyo_joint" type="fixed">
    <origin xyz="-0.1 0.0 0.08" rpy="0.0 0.0 0.0"/>
    <parent link="chassis"/>
    <child link="hokuyo"/>
    <axis xyz="0.0 1 0.0"/>
    <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
  </joint>

  <link name="hokuyo">

    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="0.00001"/>
      <inertia ixx="1e-6" ixy="0.0" ixz="0.0" iyy="1e-6" iyz="0.0" izz="1e-6"/>
    </inertial>

    <visual name="">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <mesh filename = "package://my_robot/meshes/hokuyo.dae"/>
      </geometry>
    </visual>
    
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
    </collision>
  </link>

  <gazebo reference="front_right_wheel"> 
    <material>Gazebo/Black</material> 
  </gazebo>

  <gazebo reference="rear_right_wheel"> 
    <material>Gazebo/Black</material> 
  </gazebo>

  <gazebo reference="front_left_wheel"> 
    <material>Gazebo/Black</material> 
  </gazebo>

  <gazebo reference="rear_left_wheel"> 
    <material>Gazebo/Black</material> 
  </gazebo>

  <gazebo reference="front"> 
    <material>Gazebo/Yellow</material> 
  </gazebo>

  <gazebo reference="chassis"> 
    <material>Gazebo/Red</material> 
  </gazebo>

  <gazebo reference="camera"> 
    <material>Gazebo/Orange</material> 
  </gazebo>

  <!--gazebo reference="rear_right_wheel">
    <mu1>1.5</mu1>
    <mu2>1.5</mu2>
  </gazebo>

  <gazebo reference="rear_left_wheel">
    <mu1>1.5</mu1>
    <mu2>1.5</mu2>
  </gazebo>

  <gazebo reference="chassis">
    <mu1>0</mu1>
    <mu2>0</mu2>
  </gazebo>

  <gazebo reference="front_caster_collision">
    <mu1>0</mu1>
    <mu2>0</mu2>
  </gazebo-->

  

</robot>